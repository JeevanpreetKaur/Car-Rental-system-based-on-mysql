<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carrentalsystem.ui.LoginWindowController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #F2F1C4;">
         <children>
            <TextField fx:id="txtEmail" layoutX="124.0" layoutY="107.0" prefHeight="25.0" prefWidth="352.0" promptText="Email Id" />
            <PasswordField fx:id="txtPwd2" layoutX="125.0" layoutY="148.0" prefHeight="25.0" prefWidth="352.0" promptText="Password" />
            <Button fx:id="btnRegister" layoutX="199.0" layoutY="215.0" mnemonicParsing="false" onAction="#btnLoginClicked" prefHeight="25.0" prefWidth="166.0" style="-fx-background-color: #59FF00;" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="194.0" layoutY="14.0" text="Login Window">
               <font>
                  <Font name="System Bold" size="31.0" />
               </font>
            </Label>
            <Label layoutX="185.0" layoutY="263.0" onMouseClicked="#redirectToLoginHomeWindow" text="Go to Login Home Window" textFill="#2312ff" underline="true">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="lblError" alignment="CENTER" layoutX="67.0" layoutY="192.0" prefHeight="17.0" prefWidth="466.0" text=" " textFill="RED" />
            <Label fx:id="lblUserName" layoutX="280.0" layoutY="74.0" text=" " textAlignment="CENTER" />
         </children>
      </Pane>
   </children>
</AnchorPane>
